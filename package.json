{
  "name": "assemblyscript-functions",
  "version": "1.0.11",
  "main": "lib/index.js",
  "repository": "https://github.com/togekk1/assemblyscript-functions.git",
  "author": "togekk1",
  "license": "MIT",
  "devDependencies": {
    "@assemblyscript/loader": "^0.19.20",
    "assemblyscript": "^0.19.20",
    "assemblyscript-json": "^1.1.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-index.json && tsc -p tsconfig-index-commonjs.json",
    "asbuild:untouched": "asc src/assembly/index.ts -b lib/wasm/main.untouched.wasm --exportRuntime --target debug",
    "asbuild:optimized": "asc src/assembly/index.ts -b lib/wasm/main.optimized.wasm --exportRuntime --target release",
    "build": "npm run tsc && npm run asbuild:optimized",
    "publish:prod": "yarn build && yarn publish --tag latest",
    "publish:next": "yarn build && yarn publish --tag next"
  },
  "releaseFiles": [
    "lib/",
    "index.d.ts",
    "index.js",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
